{
    "manifest_version":3,
    "name": "temp app",
    "description" : "temp app",
    "version": "1.0.0",
    
    "commands": {
      "_execute_browser_action": {
        "suggested_key": {
          "default": "Ctrl+Shift+F",
          "mac": "MacCtrl+Shift+F"
        },
        "description": "Opens hello.html"
      }
    },
   
    "permissions" : [
      "storage",
      "contextMenus",
      "tabs"
    
    ],
    
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content_script.js"]
      }
    ]
  }